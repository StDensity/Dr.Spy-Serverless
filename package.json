		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250121.0",
		"@types/node": "20.8.3",
		"@types/service-worker-mock": "^2.0.1",
		"discord-api-types": "^0.37.117",
		"wrangler": "^3.103.2"
